{
  "name": "jasmine-zephyr-reporter",
  "version": "1.0.0",
  "description": "A simple library of reporting automation test result to jira's zephyr plugin",
  "main": "index.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "jasmine": "npx jasmine-ts --config=spec/support/jasmine.json",
    "test": "npx ts-node lib/specs/api/useCase.ts",
    "format": "prettier --write",
    "format:all": "prettier --write \"src/**/*.ts\" index.ts",
    "lint": "tslint -p tsconfig.json"
  },
  "keywords": [
    "jasmine",
    "jira",
    "zephyr",
    "reporter",
    "automation",
    "e2e",
    "api testing",
    "unit testing"
  ],
  "author": "Yehiel Shamir",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "@types/node": "^16.6.1",
    "jasmine": "^3.8.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
